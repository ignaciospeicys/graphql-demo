type Chicken {
    id: Int
    name: String!
    age: Int
    breed: String!
    farm : Farm
    eggsLayed: Int
    eggs: [Egg]
}

type Query {
    chickens: [Chicken]!
    chickenById(id: Int) : Chicken
    chickenByName(name: String): Chicken
    chickensByBreed(breed: String) : [Chicken]!
}

type Mutation {
    addChicken(name: String, age: Int, breed: String, farmId: Int): Chicken
}